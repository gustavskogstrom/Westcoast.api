@model IList<Westcoast.web.ViewModels.CourseListViewModel>
@{
    Layout = "_Layout";
}
<section class="page">
<h1>Aktuella klaser</h1>
<div class="table-container">
    
<table>
    <thead>
        <tr>
            <th></th>
            <th>Titel</th>
            <th>Startdatum</th>
            <th>Slutdatum</th>
            <th></th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var course in Model)
        {
            <tr>
                <td></td>
                <td>@course.CourseTitle</td>
                <td>@course.StartDate</td>
                <td>@course.EndDate</td>
                <td><a asp-action="Details" asp-controller="Courses" asp-route-CourseId=@course.CourseId>More details</a></td>
                <td></td>
            </tr>
        }
    </tbody>
    <tfoot>
        <tr>
            <td>&nbsp;</td>
            <td>&nbsp;</td>
            <td>&nbsp;</td>
            <td>&nbsp;</td>
            <td>&nbsp;</td>
            <td>&nbsp;</td>
        </tr>
    </tfoot>
</table>
</div>
</section> 